@model IEnumerable<WebApplication4.Models.NaturalDisaster>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NatDisasterDescr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NatDisasterPrecautions)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NatDisasterPotentialEffects)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NatDisasterSigns)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NatDisasterDescr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NatDisasterPrecautions)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NatDisasterPotentialEffects)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NatDisasterSigns)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.NatDisasterID }) |
            @Html.ActionLink("Details", "Details", new { id=item.NatDisasterID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.NatDisasterID })
        </td>
    </tr>
}

</table>
