@model IEnumerable<WebApplication4.Models.InventoryType>

@{
    ViewBag.Title = "Index";
}

<h2>Inventory Types</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InvTypeDescr)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InvTypeDescr)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.InvTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.InvTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.InvTypeID })
        </td>
    </tr>
}

</table>
